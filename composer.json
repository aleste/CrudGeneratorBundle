{
    "name": "aleste/crud-generator",
    "description": "This bundle generates nice CRUDs with paginator, filters, export and more.",
    "keywords": ["crud", "generator", "bootstrap"],
    "homepage": "https://github.com/aleste/CrudGeneratorBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Alejandro Steinmetz",
            "email": "asteinmetz78@gmail.com"
        }
    ],
    "require": {
        "symfony/symfony": ">2.3.0, <2.5-dev",
        "sensio/generator-bundle": ">2.2.0, <2.4-dev",
        "knplabs/knp-paginator-bundle": "2.4.0",
        "lexik/form-filter-bundle": "v2.0.2",
        "doctrine/orm": ">=2.2.3,<2.5-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "3.6.*"
    },
    "autoload": {
        "psr-0": { "Aleste\\Bundle\\CrudGeneratorBundle": "" }
    },
    "target-dir": "Aleste/Bundle/CrudGeneratorBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
